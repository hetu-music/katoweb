{
  "name": "katoweb",
  "version": "1.2.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build && pnpm build:sw",
    "build:next": "next build",
    "build:sw": "pnpm exec tsx scripts/build-sw.ts",
    "build:icons": "pnpm exec tsx scripts/generate-icons.ts",
    "build:pwa": "pnpm build:icons && pnpm build:next && pnpm build:sw",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{js,ts,jsx,tsx,json,css,scss}\" \"scripts/**/*.{js,ts}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,jsx,tsx,json,css,scss}\" \"scripts/**/*.{js,ts}\""
  },
  "dependencies": {
    "@eslint/js": "^9.39.2",
    "@marsidev/react-turnstile": "^1.4.1",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.91.0",
    "framer-motion": "^12.28.1",
    "fuse.js": "^7.1.0",
    "globals": "^17.0.0",
    "lucide-react": "^0.562.0",
    "next": "16.1.4",
    "next-themes": "^0.4.6",
    "postcss": "^8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "tailwindcss-animate": "^1.0.7",
    "typescript-eslint": "^8.53.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@next/eslint-plugin-next": "^16.1.4",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^25.0.9",
    "@types/react": "^19.2.9",
    "@types/react-dom": "^19.2.3",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.4",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react-hooks": "^7.0.1",
    "prettier": "^3.8.1",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "workbox-build": "^7.4.0"
  }
}